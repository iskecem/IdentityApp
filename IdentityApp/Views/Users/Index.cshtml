@model IEnumerable<IdentityUser>


<a class="btn btn-secondary" asp-action="Create">Create User</a>
<br>
<br>
<table class="table table-sm table-bordered">
	<thread>
		<tr>
			<th>Id</th>
			<th>Name</th>
			<th>Email</th>
			<th></th>
		</tr>
	</thread>
	<tbody>
		@foreach (var user in Model)
		{
			<tr>
				<td>@user.Id</td>
				<td>@user.UserName</td>
				<td>@user.Email</td>
				<td>
					<a class="btn btn-primary btn-sm" href="">Edit</a>
					<a class="btn btn-danger btn-sm" href="">Delete</a>
				</td>
			</tr>
		}
	</tbody>
</table>
